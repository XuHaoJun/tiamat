{
  "name": "tiamat",
  "version": "0.2.0",
  "description": "Tiamat - a modern mobile gaming forum.",
  "scripts": {
    "test": "cross-env NODE_ENV=test PORT=8080 MONGO_URL=mongodb://localhost:27017/mern-test node_modules/.bin/nyc node --harmony-proxies node_modules/.bin/ava",
    "_test": "echo disable test",
    "watch:test": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "start": "rimraf build && cross-env NODE_ENV=development webpack --watch --config webpack.config.devServer.js",
    "start2": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "start:prod": "npm run start:production",
    "start:production": "cross-env NODE_ENV=production node index.js",
    "bs": "npm run clean && npm run build && npm run build:server && npm run start:prod",
    "build": "npm run clean && npm run build:all",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "build:server:client": "npm run build:client && npm run build:server",
    "build:all": "npm run build:server:client",
    "clean": "rimraf dist build",
    "slate": "rimraf node_modules && npm install",
    "lint": "eslint client server",
    "prettier:debug": "prettier --debug-check {server,client,Intl}/**/*.js",
    "prettier:write": "prettier --write  {server,client,Intl}/**/*.{js,jsx,md,json,css}",
    "prettier": "npm run prettier:write",
    "slate:migrate": "babel-node --presets @babel/preset-env -- ./server/util/slateMigrate.js",
    "sync:elasticsearch": "babel-node --presets @babel/preset-env -- ./server/util/syncElasticsearch.js",
    "publish:heroku": "git rebase master heroku && git push --force heroku heroku:master",
    "heroku-postbuild": "npm run build",
    "babel-repl": "npx babel-node --presets @babel/preset-env"
  },
  "pre-commit": {
    "run": [
      "_test"
    ]
  },
